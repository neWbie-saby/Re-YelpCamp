<% include ./partials/upper %>

<h1 style='text-align: center; padding-bottom: 10px;'>Welcome to the Sign-up Page!!</h1>

<div style='width: 30%; margin: 0 auto;' class='container'>
	<form action='/register' method='POST'>
		<div class='form-group'>
			<input class='form-control' type='text' name='username' placeholder='Username'>
		</div>
		<div class='form-group'>
			<input class='form-control' type='password' name='password' placeholder='Password'>
		</div>
		<!-- <input type='text' name='adminCode' placeholder='Admin PassCode(Optional)'> -->
		<div class='form-group'>
			<button style='position: absolute; right: 47.5%'>Sign-Up</button>
		</div>
	</form>
</div>
	
