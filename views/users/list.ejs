<% include ../partials/upper %>

<div class='container' style='width: 30%; margin: 0 auto;'>
	<h1 style='text-align: center; padding-bottom: 10px;'>Admin DashBoard</h1>
	<table class='table table-hover'>
		<tr>
			<th>Username</th>
			<th style='text-align: center'>Actions</th>
			<th> </th>
		</tr>
		<% userList.forEach(function(u){ %>
			<tr>
				<td><%= u.username %></td>
				<td><a href='/list/<%= u._id %>/edit' class='btn btn-warning'>Edit</a></td>
				<td><form action='/list/<%= u._id %>?_method=DELETE' method='POST'>
						<button class='btn btn-danger'>Delete</button>
					</form>
				</td>
			</tr>
		<% }) %>
	</table>
</div>

<% include ../partials/lower %>